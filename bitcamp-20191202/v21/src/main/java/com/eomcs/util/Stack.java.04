package com.eomcs.util;

public class Stack {
  
  private static final int DEFAULT_CAPACITY = 10;
  
  Object[] elementData;
  int size;
  
  public Stack() {
    this.elementData = new Object[DEFAULT_CAPACITY];
    this.size = 0;
  }
  
  public void push(Object value) {
    this.elementData[size++] = value;
  }
  
}
